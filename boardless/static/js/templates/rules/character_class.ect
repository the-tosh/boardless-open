<tr data-child_type="character_class" data-child_id="<%- @character_class.id %>" data-is_disabled="<%- +@character_class.is_disabled %>">
    <td><%- @character_class_counter %></td>
    <td class='js-child-title'><%- @character_class.title %></td>
    <td><%- @skills %></td>
