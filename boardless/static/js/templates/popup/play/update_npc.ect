<div id="js-update-npc-container">
	<form class="js-update-npc-form popup-form">
		<input type="hidden" name="id" value="<%- @object.id %>" />
		Name: <input type="text" name="title" value="<%- @object.title %>"/>
	</form>
	<span id="js-update-npc-form-ok-btn" class="btn btn-blue">OK</span>
	<span id="js-update-npc-form-cancel-btn" class="btn btn-blue">CANCEL</span>
	<span id="js-update-npc-form-delete-btn" class="btn btn-red right">DELETE</span>
</div>