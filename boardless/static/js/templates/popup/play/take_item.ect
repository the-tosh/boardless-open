<div id="js-update-item-container">
	<form class="js-update-item-form">
		<input type="hidden" name="id" value="<%- @object.id %>" />
		<div>Title: <%- @object.title %></div>
		<div>Durability: <%- @object.durability %></div>
	</form>
	<span id="js-take-item-form-ok-btn" class="btn btn-blue">Take</span>
	<span id="js-take-item-form-cancel-btn" class="btn btn-blue">Cancel</span>
</div>